{
  "name": "@feedcatch/js",
  "description": "Collect feedbacks from your users easily.",
  "version": "0.1.0",
  "main": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "*"
  ],
  "license": "MIT",
  "scripts": {
    "start": "webpack --config webpack.dev.js --watch",
    "serve": "http-server ./playground -p 9000",
