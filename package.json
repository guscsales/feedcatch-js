    "compile": "tsc -p .",
    "package": "rimraf *.tgz && cd dist && yarn pack && cd ../ && mv dist/*.tgz ./",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rm -rf dist",
    "format:check": "prettier --check \"**/*.{ts,tsx,md}\"",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@types/jest": "^29.4.0",
    "@types/node": "^20.5.9",
    "@types/webpack-dev-server": "^4.7.2",
    "css-loader": "^6.8.1",
    "dotenv-webpack": "^8.0.1",
    "ejs": "^3.1.9",
    "ejs-webpack-loader": "^2.2.2",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "file-loader": "^6.2.0",
    "http-server": "^14.1.1",
    "jest": "^29.4.2",
    "postcss-loader": "^7.3.3",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "svg-inline-loader": "^0.8.2",
    "ts-loader": "^9.4.4",
    "tsconfig": "*",
    "typescript": "^4.9.5",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "autoprefixer": "^10.4.13",
    "classnames": "^2.3.2",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.6",
    "ts-node": "^10.9.1"
  }
